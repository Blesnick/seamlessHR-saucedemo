{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "cypress",
            "node"
        ],
        "baseUrl": ".",
        "paths": {
            "@support/*": [
                "cypress/support/*"
            ]
        }
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ]
}